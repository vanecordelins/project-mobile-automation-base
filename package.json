{
  "name": "mobile-automation-project",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "test": "cross-env PLATFORM=android wdio run configs/wdio.local.conf.mjs",
    "test:android": "cross-env PLATFORM=android wdio run configs/wdio.local.conf.mjs",
    "test:ios": "cross-env PLATFORM=ios wdio run configs/wdio.local.conf.mjs",
    "test:browserstack": "wdio run configs/wdio.browserstack.conf.mjs",
    "test:feature": "cross-env PLATFORM=android wdio run configs/wdio.local.conf.mjs --spec",
    "test:tag": "cross-env PLATFORM=android wdio run configs/wdio.local.conf.mjs --cucumberOpts.tagExpression",
    "report": "allure generate allure-results --clean -o allure-report",
    "report:open": "npm run report && allure open allure-report",
    "appium": "appium"
  },
  "devDependencies": {
    "@wdio/allure-reporter": "^8.43.0",
    "@wdio/appium-service": "^8.25.2",
    "@wdio/browserstack-service": "^9.18.4",
    "@wdio/cli": "^8.25.2",
    "@wdio/cucumber-framework": "^9.18.0",
    "@wdio/local-runner": "^9.18.4",
    "@wdio/spec-reporter": "^9.18.0",
    "allure-commandline": "^2.24.0",
    "appium": "^2.0.0",
    "appium-uiautomator2-driver": "^4.2.7",
    "chai": "^4.3.7",
    "cross-env": "^7.0.3",
    "pactum": "^2.0.0"
  },
  "dependencies": {
    "@cucumber/cucumber": "^12.1.0"
  }
}
